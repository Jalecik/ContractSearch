<div class="container">
    <div class="row">
        {{#each products}}
        <div class="col-md-4">
            <a href="/product/{{_id}}">
                <div class="thumbnail">
                    <img src="{{image}}" alt="">
                    <div class="caption">
                        <h3>{{name}}</h3>
                        <p>{{category.name}}</p>
                        {{price}}
                    </div>
                </div>
            </a>
        </div>
        {{/each}}
    </div>
</div>